{
  "name": "honeycomb",
  "description": "Dependencies for the Honeycomb application",
  "repository": "git@github.com:ndlib/honeycomb.git",
  "dependencies": {
    "axios": "0.7.0",
    "browserify": "6.3.4",
    "browserify-incremental": "1.5.0",
    "classnames": "2.2.3",
    "eventemitter": "0.3.3",
    "flux": "2.1.1",
    "jquery": "2.2.0",
    "keymirror": "0.1.1",
    "material-ui": "0.13.0",
    "object-assign": "3.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-addons-create-fragment": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-addons-transition-group": "^0.14.7",
    "react-addons-update": "0.14.7",
    "react-scroll": "0.18.0",
