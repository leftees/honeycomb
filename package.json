    "react-dnd-html5-backend": "1.0.0",
    "reactify": "0.17.1"
  },
  "devDependencies": {
    "gulp": "3.9.0",
    "gulp-watch": "4.3.5",

    "react-tools": "0.12.2"
  },
  "engines": {
    "node": ">=5.5.0"
  },
  "scripts": {
    "test": "node ./node_modules/jest-cli/bin/jest.js"
