<div class="row">
  <div class="col-md-6">
    <%= react_component('ImageCaptionEditor', {image: f.object.image, fieldName: "section[caption]", caption: f.object.caption}, {prerender: false}) %>
  </div>
  <div class="col-md-6">
    <%= f.input :title %>

    <p><%= link_to "Add Intro" %> </p>

    <%= f.input :description, as: :text %>

  </div>
</div>


