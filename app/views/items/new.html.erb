<% page_title(t('.title'), @item.collection) %>
<%= collection_nav(@item.collection, :items) %>

<%= back_action_bar(collection_items_path(@item.collection), '#' ) %>

<%= simple_form_for [@item.collection, @item], :html => { class: 'new-item-dropzone', :multipart => true, :id => "fileupload"  } do |f| %>
  <%= render partial: 'new_form', locals: { f: f, title: t('.title') } %>
<% end %>

