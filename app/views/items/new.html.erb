<% page_title(t('.title'), @item.collection) %>
<%= collection_nav(@item.collection, :items) %>

<%= back_action_bar(collection_items_path(@item.collection), '#' ) %>

<%= simple_form_for [@item.collection, @item], :html => {  :multipart => true, :id => "fileupload"  } do |f| %>

<% end %>


<%= react_component('ReactDropzone', {
                      formUrl: url_for([@item.collection, @item]),
                      authenticityToken: form_authenticity_token
                    }) %>
