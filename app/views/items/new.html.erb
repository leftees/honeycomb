<div class="page-header">
  <h1><%=t '.title', :default => "Upload New Items" %></h1>
</div>
<div class="container">
  <div class="row">
  <%= form_for [@item.collection, @item], :html => { class: 'dropzone', :multipart => true, :id => "fileupload"  } do |f| %>
    <div class="fallback">
      <%= f.file_field :image %>
    </div>
  <% end %>
  </div>
  <div class="row">
    <div class=".col-xs-12">
      <%= link_to 'Done', collection_items_path(@item.collection), class: 'btn btn-default' %>
    </div>
  </div>
</div>
