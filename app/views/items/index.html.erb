<%= page_title(t('.title'), @collection) %>
<%= collection_nav(@collection, :items) %>

<%= render partial: 'action_bar', locals: { collection: @collection } %>

<%= render partial: "list", locals: { items: @items } %>


<%= react_component('ReactDropzone', {
                formUrl: url_for([@collection, Item.new ]),
                authenticityToken: form_authenticity_token,
                modalTitle: t('.add_new_items'),
                closeText: t('.add_new_items_close_text'),
                cancelText: t('.add_new_items_cancel_text'),
          }) %>

